<!DOCTYPE html>
<html lang="en">
<head>
    {% block page_head %}
        <meta charset="utf-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

        <title>{% block page_title %}{{ 'title.index'|trans([],'concilioSettings') }}{% endblock %}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
        <meta name="robots" content="noindex, nofollow"/>
    {% endblock %}
    {% block head_css %}
        <link href="{{ asset('bundles/setting/css/bootstrap.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/setting/css/helpers.css') }}" rel="stylesheet"/>
    {% endblock %}
    {% block head_js %}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
        <script src="{{ asset('bundles/setting/js/setting_general.js') }}"></script>
    {% endblock %}
</head>
<body>
{% block content %}
    {% include '@Setting/index.html.twig' %}
{% endblock %}
{% block footer %}
    <div class="container">
        <hr>

        <footer>
            <p>&copy; HengeBytes 2025</p>
        </footer>
    </div> <!-- /container -->
{% endblock %}
{% block footer_js %}
{% endblock %}
</body>
</html>
